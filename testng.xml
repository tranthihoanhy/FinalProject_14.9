<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite All" verbose="1">
    <test name="Regression with firefox">
        <parameter name="browserTest" value="firefox"></parameter>
        <classes>
            <class name="testsuite.editProject"/>
            <class name="testsuite.deleteProject"/>
            <class name="testsuite.screen"/>
            <class name="testsuite.filterProject"/>

        </classes>
    </test>

    <test name="Regression with edge">
        <parameter name="browserTest" value="edge"></parameter>
        <classes>
             <class name="testsuite.editProject"/>
            <class name="testsuite.deleteProject"/>
            <class name="testsuite.screen"/>
            <class name="testsuite.filterProject"/>
        </classes>
    </test>

 <test name="Regression with chrome">
        <parameter name="browserTest" value="chrome"></parameter>
        <classes>
             <class name="testsuite.editProject"/>
            <class name="testsuite.deleteProject"/>
            <class name="testsuite.screen"/>
            <class name="testsuite.filterProject"/>
        </classes>
    </test>
</suite>

